
<div class="dropdown menu" id="userMenu">
    <div id="userName" class="menu">{{{data.user.name}}}</div>
    <div id="actionList" class="dropdownList menu">
        <div id="sendEmail" class="menu">Send Email</div>
        <div id="sendText" class="menu">Send Text Message</div>
        <div id="logOut" class="menu"><a href="/logout">Log Out</a></div>
    </div>
</div>
<script>
    let menuName = `{{{data}}}`;
    menuName = JSON.parse(menuName)
    document.querySelector('#userName').innerHTML = menuName.user.name
</script>